<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yjs.example.springbootmybatis.mapper.article.ArticleMapper">
    <resultMap id="articleResult" type="com.yjs.example.springbootmybatis.Article">
        <id property="id" column="id"></id>
        <result property="title" column="title"/>
        <result property="content" column="content"/>

    </resultMap>

    <select id="findAll" resultType="com.yjs.example.springbootmybatis.Article">
        SELECT * FROM article;
    </select>

    <select id="findById" resultType="com.yjs.example.springbootmybatis.Article">
        SELECT * FROM article WHERE id = #{id}
    </select>
</mapper>